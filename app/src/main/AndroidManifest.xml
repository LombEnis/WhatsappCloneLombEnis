        android:supportsRtl="true"
        android:theme="@style/Theme.WhatsappCloneLombEnis">
        <activity android:name=".ArchivedChatsActivity"></activity>
        <activity android:name=".NewGroupCallActivity" />
        <activity android:name=".CallInfoActivity" />
